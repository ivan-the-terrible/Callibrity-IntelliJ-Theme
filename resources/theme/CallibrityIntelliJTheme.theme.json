{
  "name": "Callibrity-IntelliJ-Theme",
  "author": "Callibrity",
  "editorScheme": "/theme/CallibrityIntelliJTheme.xml",
  "dark": true,

  "colors": {
    "baseBlue": "#1d2542",
    "baseGreen": "#25582c",
    "baseOrange": "#d65b1e",
    "activeOrange": "#f6921e",
    "textForeground": "#f9f1d2"
  },

  "ui": {
    "*": {
      "background": "baseBlue",
      "foreground": "textForeground",
      "selectionBackground": "baseGreen",
      "hoverBackground": "baseOrange",
      "pressedBackground": "baseOrange",
      "inactiveBackground": "baseOrange",
      "underlineColor": "baseOrange"
    },
    "ScrollBar" : {
      "background" : "#211632ff",
      "hoverThumbColor": "activeOrange",
      "thumb": "#2b2040ff",
      "thumbColor": "baseGreen",
      "track": "#28233eff",
      "Transparent": {
        "thumbColor": "baseGreen",
        "hoverThumbColor": "activeOrange"
      },
      "Mac" : {
        "Transparent": {
          "thumbColor": "baseGreen",
          "hoverThumbColor": "activeOrange"
        },
        "hoverThumbColor": "activeOrange",
        "thumbColor": "baseGreen"
      }
    },
    "ToolWindow.Button.selectedBackground": "activeOrange",
    "Popup.borderColor": "activeOrange"
  }
}